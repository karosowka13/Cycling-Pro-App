{
	"name": "cycling-sport-app",
	"version": "0.1.0",
	"private": true,
	"main": "server.js",
	"dependencies": {
		"babel-preset-env": "^1.7.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-form-data": "^2.0.13",
		"express-validator": "^6.6.0",
		"fit-file-parser": "^1.6.18",
		"helmet": "^3.23.3",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.9.16",
		"multer": "^1.4.2",
		"pg": "^8.2.1",
		"web-push": "^3.4.4"
	},
	"babel": {
		"presets": [
			["env"]
		]
	},
	"scripts": {
		"babel-node": "babel-node --presets=latest",
		"start-dev": "nodemon --exec npm run babel-node -- ./server.js",
		"build-dev": "babel src -d dist",
		"clean": "rm -rf build && mkdir build",
		"build-babel": "babel -d ./build ./server -s",
		"build": "npm run clean && npm run build-babel",
		"start": "npm run build && node ./build/index.js"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-latest": "^6.24.1",
		"braces": "^3.0.2",
		"generate-schema": "^2.6.0",
		"nodemon": "^2.0.4"
	}
}
